{
  "root": true,
  "env": {
    "browser": true,
    "commonjs": true
  },
  "extends": ["eslint:recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "camelcase": ["off"],
    "no-undef": ["off"],
    "no-unused-vars": ["off"],
    "space-before-function-paren": ["error", "never"],
    "space-before-blocks": ["error"]
  }
}
